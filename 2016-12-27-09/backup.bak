CREATE TABLE "core_actionpicture" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "picture" varchar(100) NOT NULL, "actionid_id" integer NOT NULL REFERENCES "core_action" ("id"));
CREATE TABLE "core_actionreflink" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "link" text NOT NULL, "actionid_id" integer NOT NULL REFERENCES "core_action" ("id"));
CREATE TABLE "core_activity" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "activity_type" varchar(1) NOT NULL, "date" datetime NOT NULL, "feed" integer NULL, "question" integer NULL, "answer" integer NULL, "user_id" integer NOT NULL REFERENCES "auth_user" ("id"));
CREATE TABLE "core_comment_childcomment" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "from_comment_id" integer NOT NULL REFERENCES "core_comment" ("id"), "to_comment_id" integer NOT NULL REFERENCES "core_comment" ("id"));
CREATE TABLE "core_notification" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "createdDate" datetime NOT NULL, "notificationtype" varchar(1) NOT NULL, "isread" bool NOT NULL, "action_id" integer NOT NULL REFERENCES "core_action" ("id"), "comment_id" integer NOT NULL REFERENCES "core_comment" ("id"), "inviation_id" integer NOT NULL REFERENCES "core_invitation" ("id"));
CREATE TABLE "core_userprofile" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "date" datetime NOT NULL, "gender" varchar(1) NOT NULL, "occupation" varchar(100) NOT NULL, "address" varchar(200) NOT NULL, "createdDate" datetime NOT NULL, "last_active" datetime NOT NULL, "birthday" datetime NULL, "user_id" integer NOT NULL UNIQUE REFERENCES "auth_user" ("id"));
CREATE TABLE "core_invitation" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "content" text NOT NULL, "isaccept" bool NOT NULL, "createDate" datetime NOT NULL, "fromuser_id" integer NOT NULL REFERENCES "core_userprofile" ("id"), "touser_id" integer NOT NULL REFERENCES "core_userprofile" ("id"));
CREATE TABLE "core_comment" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "content" text NOT NULL, "createDate" datetime NOT NULL, "modified" datetime NOT NULL, "active" bool NOT NULL, "commentby_id" integer NOT NULL REFERENCES "core_userprofile" ("id"), "commentin_id" integer NOT NULL REFERENCES "core_action" ("id"));
CREATE TABLE "authtoken_token" ("key" varchar(40) NOT NULL PRIMARY KEY, "created" datetime NOT NULL, "user_id" integer NOT NULL UNIQUE REFERENCES "auth_user" ("id"));
CREATE TABLE "account_emailconfirmation" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "created" datetime NOT NULL, "sent" datetime NULL, "key" varchar(64) NOT NULL UNIQUE, "email_address_id" integer NOT NULL REFERENCES "account_emailaddress" ("id"));
CREATE TABLE "account_emailaddress" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "verified" bool NOT NULL, "primary" bool NOT NULL, "user_id" integer NOT NULL REFERENCES "auth_user" ("id"), "email" varchar(254) NOT NULL UNIQUE);
CREATE TABLE "django_site" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "name" varchar(50) NOT NULL, "domain" varchar(100) NOT NULL UNIQUE);
