INSERT INTO "core_action" VALUES(679,'Kiểm tra xem Tết này bạn tăng bao nhiêu cân rồi?','Một chuỗi ngày chỉ ăn chơi ngủ kết thúc bằng việc bụng có thêm vài ngấn mỡ, mặt thì tròn ơi là tròn rồi. Không biết mình còn khác con heo ở chỗ nào không nữa! Để tránh việc tăng cân quá mức kiểm soát thì bạn hãy nhớ có một chế độ ăn uống phù hợp và chăm tập thể dục, thể thao vào nhé.','2016-12-25 14:21:56.692023',NULL,NULL,'2016-12-25 14:21:56.692071','action/images/20_e9hdgiv.jpg',8,0);
