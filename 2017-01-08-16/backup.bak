INSERT INTO "core_action" VALUES(1016,'Chạy xe ôm miễn phí!','Những ngày gần tết , nhu cầu đi lại của mọi người rất nhiều đặc biệt là ở những nơi đông đúc như bến xe, sân ga, khu chợ. 
