INSERT INTO "core_action" VALUES(783,'Dành một ngày chủ nhật và chạy xe tới nơi xa nhất mà bạn có thể','.','2016-12-28 06:51:02.644405',NULL,NULL,'2016-12-29 01:22:27.595739','action/images/Weekend-Road-Trips-1.jpg',2,0);
INSERT INTO "core_action" VALUES(784,'Đưa em đi khắp thế gian','Tạo nên những kỷ niệm khó quên. Cuộc sống này rất ngắn ngủi vì thế hãy tạo nên những trải nghiệm và kỷ niệm của hiện tại sẽ là bước đệm để vẽ ra tương lai cho hai bạn. Du lịch dạy bạn biết cách sống có ý nghĩa hơn và tạo ra những kỷ niệm tuyệt vời bên nhau. Để một ngày nào đó, hai người có thể cùng ngồi trên ghế sofa uống trà và kể về chuyến đi như một điều gì đó thật đẹp và đáng nhớ...','2016-12-28 07:05:38.694172',NULL,NULL,'2016-12-29 03:19:52.109425','action/images/content_c3-Photo-by-Murad-Osmann-_-Natalia-Zakharova..jpg',2,1);
INSERT INTO "core_action" VALUES(785,'Hạnh phúc là bắt đầu cuộc phiêu lưu mới','Dám nghĩ, sẵn sàng làm những điều khác biệt, mới lạ sẽ giúp tuổi trẻ của bạn trở nên thú vị và sôi động hơn.','2016-12-28 07:29:01.128526',NULL,NULL,'2016-12-29 03:30:24.863913','action/images/Adventure-at-the-Mountain.jpg',2,0);
INSERT INTO "core_action" VALUES(786,'Thử thách bản thân cùng những điều mới mẻ','Những thử thách mới mẻ giúp bạn tự mình bứt phá ra khỏi những khuôn khổ, những lối mòn sẵn có, phát triển cá tính riêng của bạn, khai mở những con đường mới. Qua khó khăn, thử thách, cuộc sống mang lại cho bạn những cơ hội thật bất ngờ. Nếu không có thử thách, làm sao ta biết và nhận ra được sức mạnh tuyệt vời đang còn ẩn chứa trong chính bạn?','2016-12-28 07:41:49.032240',NULL,NULL,'2016-12-29 03:34:53.082571','action/images/Slider2_1700x713_0.jpg',2,1);
