INSERT INTO "core_action" VALUES(1025,'Một bữa ăn chay','Ăn chay đủ chất, khoa học đã trở thành một phong trào sống khỏe, an vui và đang được khuyến khích mạnh mẽ tại khắp nơi trên thế giới.
