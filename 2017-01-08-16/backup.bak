INSERT INTO "core_action" VALUES(872,'Hát nghêu ngao giữa đường','Đừng ngần ngại, cất tiếng hát nghêu ngao trên con đường đầy mộng mơ nhé! Cảm giác như thiên nhiên đang là của bạn. Mặc kệ mọi thứ xung quanh chỉ để tiếng hát hòa quyện với thiên nhiên và đất trời để ta được thoải với thời gian. Làm những gì ta thích và yêu những gì ta làm!!!','2016-12-29 07:51:37.960374',NULL,NULL,'2017-01-03 16:29:24.396791','action/images/3_6H140Qr.png',11,1);
