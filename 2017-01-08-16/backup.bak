INSERT INTO "core_action" VALUES(900,'Bị lạc ở một thành phố lạ','Còn thì thích thú bằng việc lang thang rồi bỗng dưng bị "Lạc" ở một thành phố lạ trong khi không quen biết ai, sau đó là tha hồ mà khám phá và tìm hiểu mà không lo gì cả, vì mình đã lạc rồi mà.','2016-12-29 15:46:12.911288',NULL,NULL,'2016-12-29 16:14:00.428012','action/images/lang_thang_trong_thành_phố.png',8,1);
