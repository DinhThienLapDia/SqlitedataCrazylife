INSERT INTO "core_action" VALUES(791,'Hạnh phúc là khi nhìn thấy bố mẹ cười','Có những lúc bạn sẽ chẳng thể biết mình đã có những gì cho đến khi nó mất đi vĩnh viễn. Bạn tặc lưỡi, lúc nào về nhà cũng được, dễ thôi có gì to tát đâu.... Ấy vậy mà, đã bao nhiêu cái cuối tuần bạn bận rộn với công việc, bận cà phê với bạn bè, bận lang thang tại những nơi thật xa, mà bạn quên đi có những người chỉ muốn nhìn thấy bạn bạn cười , cùng ăn bữa cơm, hỏi han vài ba câu chuyện về công việc, về cuộc sống thì những nếp nhăn bởi thời gian trên khuôn mặt họ dường như chẳng còn nữa. Bạn có thể đi đến bất kì nơi đâu, nhưng để dùng hai từ "trở về" thì chi có duy nhất gia đình mà thôi...Cuối tuần này bạn có kế hoạch gì chưa? Còn tôi, sẽ bắt chuyến xe cuối ngày, và về ăn bữa cơm bên gia đình!','2016-12-28 08:49:19.934548',NULL,NULL,'2016-12-29 04:07:22.131860','action/images/Apresentação1.jpg',2,1);
INSERT INTO "core_action" VALUES(792,'Mặc chiếc áo phông có hình band nhạc bạn yêu thích','Không gì thể hiện cá tính của bạn rõ ràng và thoải mái hơn những chiếc áo phông mang màu sắc đặc trưng của nhóm nhạc mà bạn yêu thích, và với tên ban nhạc hay tên các thành viên in trên áo, bạn chẳng tội gì mà phải chạy đua với các loại áo phông in slogan bán tràn lan trên thị trường. Nhiều nhóm nhạc còn có hình logo thiết kế đẹp chẳng kém gì các họa tiết thời trang thịnh hành. Với ưu thế về tính tiện lợi và thời trang như thế, một chiếc áo phông band nhạc chắc chắn sẽ giúp bạn thoải mái và tự tin hơn rất nhiều!','2016-12-28 09:03:23.344064',NULL,NULL,'2016-12-29 04:14:07.618144','action/images/How-To-Wear-Rock-Chic-Band-T-Shirts-3.jpg',2,1);
