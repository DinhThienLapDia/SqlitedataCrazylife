INSERT INTO "core_action" VALUES(934,'Tình yêu là cùng nhau đọc những cuốn sách','Khi yêu một ai đó, hai người có thể làm hai việc hoàn toàn khác nhau nhưng không bao giờ là đơn độc. Như khi cùng nhau đọc sách, mỗi người đọc một quyển sách khác nhau nhưng hai bên vẫn cảm nhận được sự ấm áp từ nửa kia đang ở bên cạnh mình. Ngay cả khi đắm chìm vào thế giới trong cuốn tiểu thuyết nhưng hai người vẫn ở đó bên nhau để cho nhau biết mình quan tâm đến người kia nhiều như thế nào.','2016-12-30 06:25:52.441019',NULL,NULL,'2017-01-03 09:25:44.299297','action/images/tumblr_m1sl6ypMb11qj5bgno1_500.jpg',2,1);
