INSERT INTO "core_action" VALUES(711,'Tuổi thơ là những lần lên và những lần xuống','Bạn lên – tôi xuống
Bạn xuống – tôi lên
Bập… bênh, bập… bênh
Thích ơi là thích.
Muốn cho đều nhịp
Phải giúp đỡ nhau
Nặng – thì xuống mau
Nhẹ - thì lên chóng
Bàn chân đạp xuống
Là lược tôi lên
Cứ thế hai bên
Chẳng ai sau, trước
Chẳng ai thua được
Cùng chơi cơ mà....
Cho tôi xin một vé về tuổi thơ!','2016-12-26 03:51:29.219624',NULL,NULL,'2016-12-27 03:56:13.698397','action/images/002113.jpg',2,0);
