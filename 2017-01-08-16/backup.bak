INSERT INTO "core_action" VALUES(868,'Đặt chân đến miền tây','Mảnh đất giản dị, thân thương ở Miền Tây với những hệ thống sông ngòi, kênh rạch, vườn cây ăn trái, cánh đồng rộng mênh mông, chợ nổi, món ăn... là điểm đến lý tưởng. Miền Tây sẽ giúp mình có những thời gian được giảm căng thẳng, hiểu được văn hóa của dân tộc mình.','2016-12-29 07:46:47.610231',NULL,NULL,'2016-12-29 21:35:39.300043','action/images/14_TjrjzyQ.png',11,1);
INSERT INTO "core_action" VALUES(869,'Đi máy bay','Trải nghiệm "bay" khiến bạn như được nhất lên khỏi mặt đất, trên cao nhìn xuống từ từ cảm nhận vạn vật trở nên nhỏ bé. Một cảm giác chắc chắn bạn nên có một lần trong đời đấy nhé!
Hãy lên lịch một ngày gần nhất để bay lên bầu trời nhé!','2016-12-29 07:47:59.515937',NULL,NULL,'2017-01-03 16:50:12.755582','action/images/4_3DEwR2z.png',11,1);
