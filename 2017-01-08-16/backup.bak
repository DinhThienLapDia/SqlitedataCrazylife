INSERT INTO "core_action" VALUES(893,'Móc một chiếc khóa vào một nơi nào đó, một chiếc khóa khắc tên bạn và người ấy','Một cặp đôi đến từ Hà Tĩnh chia sẻ đầy hào hứng: “Mình xem nhiều hình ảnh về cầu tình yêu ở nước ngoài và đây là lần đầu tiên chúng mình có cơ hội được đứng trên 1 cây cầu như vậy. Cảm giác khi cùng nhau móc ổ khóa tình yêu vào thành cầu rất đặc biệt và xúc động. Đây sẽ là 1 kỷ niệm đáng nhớ. Chắc chắn sau này chúng mình sẽ quay lại đây để hồi tưởng lại những ký ức đẹp”','2016-12-29 15:17:21.579906',NULL,NULL,'2017-01-04 04:05:39.176198','action/images/123457.jpg',2,1);
