INSERT INTO "core_action" VALUES(619,'Trải nghiệm làm thử một sản phẩm về gốm','Những sản phẩm từ gốm, sứ xuất hiện rất rất nhiều trong cuộc sống hàng ngày của mỗi chúng ta. Từ những chiếc bát, đĩa cho đến cốc, chén. Có bao giờ bạn tự hỏi: Những sản phẩm như vậy được làm ra như thế nào? Không khó để tìm câu trả lời khi bạn có thể dành ra ngày cuối tuần tham quan và trực tiếp tạo ra một sản phẩm cho riêng mình ở những địa điểm ngay gần thủ đô Hà Nội như Láng Gốm Bát Tràng, Gia Lâm, Hà Nội. Nhắn tin cho mấy đứa bạn thân và lên kế hoạch ngay thôi!','2016-12-25 08:30:12.165479',NULL,NULL,'2017-01-07 02:49:14.180825','action/images/02.jpg',8,1);
INSERT INTO "core_action" VALUES(620,'Trải nghiệm một ngày làm nông dân','Trong cuộc sống hiện đại ngày nay, một ngày của chúng ta bị gói gọn trong công việc, học tập cùng với vô vàn thiết bị điện tử, khiến cho đầu óc trở nên mệt mỏi. Vậy tại sao ta không thử dành ra đôi ngày cuối tuần, về miền quê và trải nghiệm công việc của một nông dân thực thụ, để ta trân quý hơn từng hạt gạo, từng sản phẩm mà chúng ta đang sử dụng hôm nay. Đó mới chính là trải nghiệm mà mỗi người cần có trong cuộc sống.','2016-12-25 08:30:38.386145',NULL,NULL,'2017-01-07 02:49:06.976297','action/images/MOT-NGAY-LAM-NONG-DAN-TAI-NONG-TRAI-TREE.JPG',8,1);
