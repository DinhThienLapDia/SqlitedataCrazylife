INSERT INTO "core_action" VALUES(818,'"Khởi động" cho một nụ hôn Eskimo','Đây được gọi là kiểu hôn của người Eskimo. Hai mũi chà nhẹ lên nhau, trong khi cả hai cùng nhắm mắt tận hưởng không khí thân thiết dịu dàng và trẻ thơ. Đây là cách bày tỏ tình yêu giản dị nhưng sâu sắc.','2016-12-28 13:28:05.041846',NULL,NULL,'2016-12-29 05:12:56.003808','action/images/eskimo-kiss-e1378631873737.jpg',2,1);
INSERT INTO "core_action" VALUES(819,'Có một cuốn sổ tay để lưu lại ý tưởng ở khắp mọi nơi','Những ý tưởng thường sẽ không đến từ những lúc bản thân phải tập trung làm việc, mà lại hay xuất hiện thì chúng ta thoải mái, lặng nhìn mọi thứ xung quanh. Chính vì vậy, có một cuốc sổ tay theo người là cách tốt nhất để ta lưu những ý tưởng hay ho đó lại để có thể suy nghĩ và phát triển thêm sau này.','2016-12-28 13:28:24.484432',NULL,NULL,'2016-12-28 13:28:24.484480','action/images/writing.jpg',8,1);
INSERT INTO "core_action" VALUES(820,'Gửi tặng đứa bạn thân một chiếc bưu thiếp thật đẹp','Một chiếc bưu thiếp thật đẹp kèm theo những lời nhắn nhủ của bạn được chuyển bất ngờ tới đứa bạn thân chắc chắn sẽ khiến nó bất ngờ và cảm động phát khóc ấy chứ. Tham khảo một số bưu thiếp đẹp tại đây nhé{http://vforum.vn/diendan/showthread.php?49339-Mau-thiep-giang-sinh-handmade-dep-3D-Tu-lam-thiep-Noel}','2016-12-28 13:35:05.621339',NULL,NULL,'2016-12-28 13:35:05.621411','action/images/Vintage-palace-3D-laser-cut-pop-up-giấy-handmade-bưu-thiếp-tugrave.jpg',8,1);
INSERT INTO "core_action" VALUES(821,'Một nụ hôn ngọt ngào giữa dòng người vội vã','Đôi khi chỉ cần một nét cười, cái ôm, nụ hôn... cũng đủ để biểu hiện cảm xúc bạn dành cho người kia say đắm, chân thành đến mức nào.','2016-12-28 13:38:03.406840',NULL,NULL,'2016-12-29 06:57:34.841779','action/images/photographer-captures-couples-kissing-in-public-652x400-1-1455524416.jpg',2,1);
INSERT INTO "core_action" VALUES(822,'Có một chiếc hộp để lưu giữ những kỷ niệm','Những vật lưu niệm vô giá sẽ được cất kỹ trong một chiếc hộp riêng, nhỏ nhắn và gọn gàng thích hợp cho việc lưu giữ và khỏi bị thất lạc. Để rồi mỗi khi cảm thấy nhớ, chỉ việc đơn giản lôi cái hòm ra, trầm ngâm mơ mộng về một thời đã qua lâu mà sao như vẫn hiển hiện ngay đây trong tâm trí vậy','2016-12-28 13:42:11.521965',NULL,NULL,'2016-12-28 13:42:11.522015','action/images/Wedding-Gift-2.jpg',8,1);
