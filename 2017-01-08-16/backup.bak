INSERT INTO "core_action" VALUES(439,'Nhớ dọn nhà vào cuối tuần nhé!','"Nhà sạch thì mát, bát sạch ngon cơm." Một không gian sống khang trang, sạch đẹp chắc chắc sẽ giúp bạn cảm thấy thoải mái,  có thêm cảm hứng làm việc và luôn mong muốn trở về nhà sau một ngày làm việc mệt mỏi đó. Bắt tay vào công việc ngay thôi.','2016-12-25 03:30:32.853256',NULL,NULL,'2016-12-26 15:04:39.309468','action/images/9_5vJOJSP.jpg',8,1);
INSERT INTO "core_action" VALUES(440,'Bí mật tổ chức tiệc kỷ niệm ngày cưới cho ba mẹ','Nhiều khi những bận rộn, toan tính của công việc hằng ngày đã làm ba mẹ quên đi cuộc sống riêng tư, đầm ẩm của bản thân để lo cho chúng con, có một điều kiện sinh hoạt tốt hơn, đầy đủ hơn những gì ba mẹ đã phải trải qua trong quá khứ. Chúng ta, đừng chỉ biết suy nghĩ cho riêng bản thân, mà hãy một lần quan tâm hơn đến niềm vui nho nhỏ của cha mẹ. Dịp kỷ niệm đám cưới của ba mẹ lần này nhất định phải tổ chức một bữa tiệc để làm họ bất ngờ mới được. Lên kế hoạch chuẩn bị thôi nào','2016-12-25 03:30:42.011115',NULL,NULL,'2016-12-26 15:04:54.214451','action/images/10.jpg',8,1);
