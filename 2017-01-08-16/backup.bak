INSERT INTO "core_action" VALUES(617,'Hôn trộm người ấy','Đối với những cặp đôi yêu nhau, việc giữ cho tình cảm giữa hai người luôn tươi mới và kéo dài lâu luôn là mối bận tâm lớn nhất. Đôi khi những thứ có giá trị lớn về mặt vật chất, tiền bạc lại không làm được nhiệm vụ ấy, mà chính những hành động nhỏ, như một tin nhắn nhắc người chúng ta thương hãy nhớ mặc ấm khi ra đường buổi sáng mùa đông, hãy tự mình mua đồ ăn đến nhà cho họ khi họ ốm, hay chi là một nụ hôn trộm khi ta dạo bước cùng nhau lại chứa đựng nhiều hơn chúng ta tưởng. Thêm một chút lắng nghe, thêm một chút quan tâm, khoảng cách giữa hai người gần sẽ càng gần hơn bao giờ hết.','2016-12-25 08:28:57.615440',NULL,NULL,'2017-01-07 02:49:20.664968','action/images/world_kiss_day_HD_wallpapers_24.jpg',8,1);
