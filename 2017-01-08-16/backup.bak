INSERT INTO "core_action" VALUES(642,'Được ăn uống miễn phí thả ga một bữa','Nếu còn là sinh viên chắc hẳn những ngày cuối tháng là những ngày khá khó khăn với bạn. Thế nên một giải pháp "tình huống" lúc đó là đi ăn trực lũ bạn. Đây là một dịp tốt để bạn tiếp kiệm chi phí lại có thể học thêm kỹ năng "mặt dày" nữa. Thật là quá tuyêt phải không?','2016-12-25 09:02:57.531807',NULL,NULL,'2017-01-05 09:04:38.878931','action/images/20.jpg',8,1);
