INSERT INTO "core_action" VALUES(473,'Ngắm sao băng và cầu nguyện','Đã từ xa xưa, sao băng luôn là một trong những hiện tượng thiên văn mà con người hằng mong muốn khám phá. Vẻ đẹp cùng sự bí ẩn của những trận mưa sao băng khiến không ít người mong được nhìn thấy một lần trong đời và hi vọng những điều mà mình cầu nguyện hay ước khi sao băng rơi thì điều ước sẽ trở thành hiện thực.','2016-12-25 03:44:55.296065',NULL,NULL,'2016-12-25 03:44:55.296120','action/images/ezgif.com-resize_GNWnQTw.gif',2,1);
