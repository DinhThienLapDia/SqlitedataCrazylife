CREATE TABLE "core_action" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "title" varchar(1024) NULL, "content" text NOT NULL, "createDate" datetime NOT NULL, "startDate" datetime NULL, "endDate" datetime NULL, "modified" datetime NOT NULL, "firstPicture" varchar(100) NOT NULL);
INSERT INTO "core_action" VALUES(3,'Chuẩn bị những phong bao lì xì may mắn ngày Tết','Tết vui, Tết sum vầy, Tết là phải có bánh chưng, bát Tét. Nhưng Tết cũng không thể thiếu chuyện mừng tuổi cho nhau, chúc nhau may mắn, sức khỏe, giàu có, sung túc. Từ trẻ con, người già, thậm chí là những người trung tuổi đều được nhận phong bao lì xì thể hiện sự may mắn.

Người Việt bao đời nay vẫn giữ nguyên phong tục truyền thống ấy và nhất nhất không thể nào quên mỗi dịp đi chơi, gặp gỡ người khác khi xuân về.

Bạn có thể tự tay làm những chiếc lì xì để gửi gắm sự chân thành của mình hoặc đi sắm tết và chọn những phong bao lì xì may mắn với nhiều mẫu mã khác nhau.

Chi tiết tham khảo thêm:
Các mẫu bao lì xì handmade năm 2017 {http://vietgiftcenter.com/phong-bao-li-xi-dep-doc-handmade-tet-binh-than-2016}
Nguồn gốc và ý nghĩa của phong tục lì xì ngày Tết {https://www.youtube.com/watch?v=ul6RVxANqns}','2016-12-09 13:00:27.021288','2017-01-12 12:59:40','2017-01-27 12:00:00','2016-12-09 13:01:50.164699','action/images/Laisee.jpg');
INSERT INTO "core_action" VALUES(4,'Tết năm nay bạn đã có những dự định gì cho bản thân và gia đình rồi?','Trong không khí của tết nguyên đán đang rạo rực khắp các phố phường, bạn đã có một kế hoạch cụ thể nào cho tết nguyên đán sắp tới chưa?
Hãy để dịp tết này sẽ luôn ngập tràn trong niềm vui và tiếng cười. Bạn định về thăm quê hay là chọn cách ăn tết ở một thành phố lạ?  Bạn muốn thăm thú các lễ hội cổ truyền hay đơn giản là đi cầu ăn ở ngồi chùa làng cổ kính? Có rất nhiều lựa chọn để bạn có thế trải nghiệm và chắc chắc nó sẽ là những kỷ niệm không thể nào quên của bạn đó.','2016-12-09 13:06:10.985114','2016-12-09 12:57:03','2017-01-31 12:57:11','2016-12-09 13:53:49.218808','action/images/1111111111_cNNIicw.jpg');
INSERT INTO "core_action" VALUES(8,'Cắt tóc đón năm mới','Hiện nay đang là thời điểm vừa là những ngày thường bận rộn vừa là những ngày chúng ta chờ đợi đón năm mới.  Hẳn là các bạn ai cũng đang háo hức chuẩn bị quần áo đẹp, làm mới bản thân chuẩn bị đón Tết nhưng các bạn cũng đừng quên lựa chọn cho mình những mẫu tóc đẹp để thật tự tin và nổi bật khi du xuân nhé.
Có rất nhiều mẫu tóc đẹp cho bạn lựa chọn để diện đi chơi trong dịp Tết này. 
Chi tiết tham khảo thêm
Các kiểu tóc đẹp cho các bạn gái dịp Tết {http://www.lamsao.com/cac-kieu-toc-dep-de-chi-em-dieu-da-don-tet-p214a99898.html}
Các kiểu tóc đẹp cho nam giới dịp Tết {http://www.biendao24h.vn/products/Kieu-toc-nam-truyen-thong-ton-tai-mai-voi-thoi-gian.html}','2016-12-09 14:03:11.532733','2016-12-01 07:03:22','2017-01-27 05:00:00','2016-12-09 14:23:49.111820','action/images/1389599906-img_0394.jpg');
INSERT INTO "core_action" VALUES(9,'Tự mình chuẩn bị một bình hoa đẹp','Mỗi dịp Tết đến xuân về, kháo nhau tìm mua hoa tươi sớm để cắm hoa Tết cho ngôi nhà thêm tươi mới. Theo tâm linh đặt hoa tươi trong nhà có ý nghĩa cầu mong một năm mới khởi sắc với nhiều may măn, đầy đủ. Tùy theo từng loại hoa khác nhau sẽ có ý nghĩa khác nhau. Nếu các năm thường mẹ hay chị sẽ làm điều đó tại sao năm nay bạn không tự tay mình chuẩn bị một bình hoa như thế nhỉ?
 Từ việc chọn hoa mua hoa hay cắm hoa đều là những công việc rất thú vị vừa thể hiện được gu thẩm mỹ của bạn nữa, hãy xách mông lên và cắm ngay một bình hoa theo hướng dẫn thôi nào
 
chi tiết đây nhé mọi người {https://tuyentaphay.com/tuyen-tap-cac-mau-cam-hoa-de-ban-ngay-tet-mang-loc-vao-nha}','2016-12-09 14:03:40.107253',NULL,NULL,'2016-12-09 14:37:41.383224','action/images/cuu-nguoi-mau-thuy-hanh-day-con-cam-hoa-ngay-tet.jpg');
INSERT INTO "core_action" VALUES(10,'Đầu năm mới bạn đã đi  lễ chùa cầu an cho bản thân và gia đình chưa?','Lễ Chùa đầu năm đã trở thành một nét văn hóa không thể thiếu của mỗi con người Việt Nam mỗi dịp tết đến xuân về. Bên cạnh những hoạt động vui chơi giải trí, những lễ hội tưng bằng sắc xuân thì không khí linh thiêng, thanh tịnh ở những đền, miếu cũng là hoạt động thu hút rất nhiều bạn trẻ tham gia với lý do:" Cầu một năm mới may mắn, bình yên, hạnh phúc"','2016-12-09 14:09:32.514207','2016-12-09 07:06:01','2017-01-31 07:06:08','2016-12-09 14:09:32.514347','action/images/images923115_5.JPG');
INSERT INTO "core_action" VALUES(11,'Nếu có ghé qua các đền chùa vào dịp tết, đừng quên xin một quẻ bói xem tử vi của năm mới','Đi lễ chùa đầu năm và xin quẻ là phong tục lâu đời của người Việt. Theo tục cũ, người đi chùa sau khi dâng lễ chọn lấy một quẻ thẻ thường làm bằng tre có ghi số hiệu hoặc ghi một câu ngắn gọn bằng tiếng Hán nêu tổng quát về cuộc đời, vận hạn của người rút quẻ trong năm đó. Ngày nay, thay vì thẻ tre, các chùa thường dùng thẻ giấy và chữ quốc ngữ để người xem dễ hiểu, không cần nhờ đến các thầy đồ luận giải. 
Tại một số đình chùa, người đi lễ được rút thẻ miễn phí. Nội dung các quẻ thẻ tập trung nói đến vận hạn của mọi người trong năm. Quẻ tốt hay xấu thể hiện ngay ở tên quẻ: quẻ đại cát, quẻ thượng thượng, quẻ hạ hạ.... Cũng có những quẻ luận giải cụ thể các vấn đề hôn nhân, cầu tài, cầu danh, gia trạch.... và tổng giải.
Người đi lễ chùa rút quẻ đầu năm xuất phát từ mong muốn sẽ gặp nhiều may mắn, an lành trong cả năm trước mắt. Đây là phong tục đã được nhân dân ta lưu giữ từ lâu và trở thành nét đẹp văn hóa mỗi dịp tết đến, xuân về. 
Dẫu vậy nhưng những điều ta xem được chưa chắc đã đáng tin, thế nên nếu không mang có gặp phải quẻ xui thì cũng đừng vì thế mà lo lắng nha.','2016-12-09 14:22:24.567539','2016-12-09 14:21:43','2017-01-31 14:21:48','2016-12-09 14:22:41.399951','action/images/gieo-que3.png');
INSERT INTO "core_action" VALUES(12,'Sắm cây quất ngày Tết','Cây quất thường được người dân lựa chọn để trang trí vào dịp Tết cũng bởi theo âm Hán Việt thì âm của từ "quất" gần giống âm của từ "cát". Quất có lá xanh tốt, sai quả, quả vàng đều thể hiện sự trù phú, hứa hẹn một năm mới được mùa, ăn nên làm ra, dồi dào sức sống. 
    Chính vì vậy, mỗi khi Tết đến xuân về, ngoài việc xem ngày tốt xấu để dọn dẹp nhà cửa đón Tết, nhà nhà đều bài trí cây quất với hy vọng mang lại niềm vui và may mắn trong cả năm cho gia đình. Ngoài ra, cành quất trĩu quả trong quan niệm dân gian là biểu tượng cho sức khỏe, bình an, trường thọ và sự may mắn trong tình duyên, thể hiện sự sum vầy của nhiều thế hệ trong gia đình. Những cây quất lá xanh tốt, quả vàng chi chít thể hiện sự trù phú, hứa hẹn năm mới được mùa, ăn nên làm ra, dồi dào sức sống. Bài trí cây quất trong nhà với hy vọng mang lại niềm vui và may mắn cả năm cho gia đình, trong kinh doanh, đặt cây quất ở văn phòng hoặc cửa hàng sẽ đem lại cát khí lớn lao, sự đầu tư sáng suốt và thu được nhiều tài lộc.

Chi tiết tại {http://saigonhoa.com/product/cay-quat-cay-tac-ngay-tet/}','2016-12-09 14:34:54.098328',NULL,NULL,'2016-12-09 14:34:54.098448','action/images/Manhhv2.jpg');
INSERT INTO "core_action" VALUES(13,'Tết này chọn Đào nào đây !!!???','Cây đào được xem là tinh hoa của Ngũ hành, theo phong thủy cây này có thể trị bách quỷ hơn nữa hoa đào còn là biểu tượng cho sự đổi mới và sức sinh sôi phát triển mạnh mẽ. 
Vì vậy sắm một cây đào tết thật đẹp để tô điểm cho Tết năm nay thôi !

Kinh nghiệm chọn và chăm sóc hoa đào ngày Tết: 
Chọn đào

Chơi đào ngày Tết là cả một nghệ thuật cao cấp từ người trồng cho đến người mua, phụ thuộc vào nhiều yếu tố: kỹ thuật trồng, chăm sóc, điều kiện kinh tế, sở thích, loại đèn, màu tường, không gian rộng hay hẹp và cách trang trí phòng khách… của mỗi hộ gia đình mà chọn lựa cho phù hợp. Đào có nhiều giống: đào bích, đào phai, đào trắng (bạch đào, rất hiếm); có đào thế, đào cảnh trồng chậu, đào cắt cành cắm lọ…
Xem thêm: {http://vietnamnet.vn/vn/doi-song/kinh-nghiem-chon-va-cham-soc-hoa-dao-ngay-tet-220248.html}','2016-12-09 14:38:15.968350',NULL,NULL,'2016-12-09 14:39:26.747366','action/images/duan-00001.jpg');
INSERT INTO "core_action" VALUES(15,'Mọi người sẽ rất bất ngờ khi nhận được lời chúc viết tay của bạn đó','Mỗi một tấm thiệp mang những lời chúc sức khỏe, may mắn và bình an của bạn sẽ làm những  người thân trong gia đình cảm thấy cảm động và hành phúc viết bao. Hãy để món quà nhỏ này là một điều bất ngờ cho mọi người bạn nhé!!','2016-12-09 14:43:09.812813','2016-12-09 14:25:47','2017-01-31 14:26:35','2016-12-09 14:43:44.130347','action/images/son0001.jpg');
INSERT INTO "core_action" VALUES(16,'Có một buổi liên hoan nhỏ đầm ấm đón chờ khoảnh khẳn giao thừa','Đêm Giao thừa là một buổi tối đặc biệt. Đó là một buổi tối thật ấn tượng. Nó là thời điểm của năm mà bạn có thể bỏ lại hết những nỗi đau buồn trong năm qua và đón chào những giấc mơ và hoài bão của chúng ta trong năm tới. Chúng ta cũng có thể nhìn lại những điều tốt đẹp mà chúng ta đã làm được, những người bạn mới, những kỳ lễ hội và những thành công trong công việc mà chúng ta đã có. 

Vì vậy hãy tổ chức buổi tiệc theo phong cách của bạn. Cố thêm vào những suy nghĩ của bạn. Nó không quá khó để làm. Bạn có thể nhớ lại những việc đã xảy ra trong năm qua với những kỷ niệm đáng nhớ. Bạn đã có chuyến du lịch đáng yêu? hay chuyện cưới hỏi? chuyện thăng tiến trong công việc? hay gặp được người yêu? Bây giờ là lúc để gợi lại những kỷ niệm. Hãy hâm nóng những gì còn lạnh lẽo trong lòng. Hãy làm cho buổi tiệc của bạn thật lãng mạn và gây tác động mạnh mẽ. Hãy lên chủ đề cho buổi tiệc dựa vào những cảm giác của bạn.

Điều quan trọng nhất là buổi tiệc của bạn phải mang tính cá nhân. Bạn sẽ phải ngạc nhiên vì chúng có tác động rất mạnh đến những vị khách của bạn. f','2016-12-09 14:51:24.326811',NULL,NULL,'2016-12-09 14:51:24.326863','action/images/son0002.jpg');
INSERT INTO "core_action" VALUES(17,'Mình còn mừng tuổi ông bà được bao nhiêu lần nữa vậy ba?','Ông bà luôn cho ta sự ấm áp nhất, vì vậy bạn đừng quên dành thật nhiều tình cảm cho ông bà của mình không chỉ riêng vào các ngày lễ Tết. Mừng tuổi ông bà là một điều hết sức ý nghĩa đối với ông bà!
Hãy để ông bà cảm nhận được sự quan tâm của con cháu dành cho mình, như vậy ông bà mới có thể vui khỏe để sống thật lâu cùng  với con cháu. Ở cái tuổi xế bóng này, sự cô đơn chính là nỗi ám ảnh sợ hãi lớn nhất, là một người cháu hiếu thảo bạn hãy xoa dịu sự cô đơn trong lòng tuy chỉ cần một cái quan tâm thật nhỏ bé nào đó. 
Đừng quên điều này nhé !
--------
Crazylife.co','2016-12-09 14:59:35.579639',NULL,NULL,'2016-12-09 14:59:35.579702','action/images/duan-00002.jpg');
INSERT INTO "core_action" VALUES(18,'Gửi những lời chúc chân thành đến cha mẹ','Tết là dịp của gia đình đoàn tụ đón giây phút giao thừa thiêng liêng... và cũng là dịp để gửi những lời chúc Tết, chúc mừng năm mới tới bố mẹ người đã sinh thành, nuôi dạy chúng ta. Cùng gửi đến những lời chúc năm mới tốt đẹp nhất đến bố mẹ kính yêu nào.

Xem thêm tại
Những lời chúc bố mẹ hay nhất{http://www.lamsao.com/nhung-loi-chuc-tet-bo-me-hay-nhat-p214a61005.html}','2016-12-09 15:00:50.162466',NULL,NULL,'2016-12-09 15:20:33.764587','action/images/Son0003.jpg');
INSERT INTO "core_action" VALUES(19,'Tạo một căn nhà tràn ngập màu sắc với đèn và bóng nháy','Một căn phòng đầy ánh sáng đủ màu hay một khung cảnh ảo diệu sẽ hiện ra trong dịp tết này khi bạn sử dụng những đèn hay những bộ bóng nháy đủ màu
  Tết là ngày để mọi người vui chơi sau một năm làm việc nên công việc trang hoàng nhà cửa sẽ không thể thiếu. Hãy chứng tỏ với mọi người là bạn là người sáng tạo đi nào. Không chỉ nhà cửa mình có thể dùng đèn náy trang trí cây mai, cây đào hay là cờ tổ quốc.

hướng dẫn chi tiết {http://bellasofa.vn/khong-gian-lung-linh-voi-nhung-chiec-den-nhay-lap-lanh.html}','2016-12-09 15:18:21.289689',NULL,NULL,'2016-12-09 15:21:34.313074','action/images/manhhv2.jpg');
INSERT INTO "core_action" VALUES(20,'Trông nồi bánh chưng ngày Tết','Ngày Tết cổ truyền hàng năm, có nhà ai là không có dăm cặp bánh chưng để cúng gia tiên và thưởng thức hương vị cổ truyền ? 
Bánh chưng là nét văn hóa lâu đời mà có lẽ mãi mãi về sau cũng không thể biến mất trong tâm thức người Việt. Nét truyền thống đó sẽ còn mãi với hình ảnh nồi bánh chưng sôi sùng sục suốt đêm và những ánh lửa bập bùng, rạng rỡ trong đôi mắt người trông bánh. Ngồi bên bếp than hồng đến cay xè cả mắt, ăn khoai lang nướng đen xì những đôi môi, nghe bố kể chuyện tết xưa, ríu rít nói chuyện với anh chị em và thi thoảng còn ngủ gật nữa. Tảng sáng, mùi bánh đã thơm lừng, quyện trong hương thơm mùi già ngai ngái ngon tuyệt…','2016-12-09 15:19:50.231638','2016-12-09 15:19:24','2017-01-27 05:00:00','2016-12-09 15:19:50.231780','action/images/Khanh75123.jpg');
INSERT INTO "core_action" VALUES(21,'"Mồng ba tết thầy"','Sang ngày mồng ba, người Việt thường dành riêng đi lễ Tết thầy giáo. Không chỉ có các học trò học chữ đến với thầy dạy chữ mà thầy được người Việt mở rộng nghĩa là những bậc có công dạy chữ lẫn dạy nghề, dạy cả những bộ môn nghệ thuật như dạy đàn, dạy hát…

Người Việt có một câu thành ngữ chân phương, nôm na nói về vai trò người thầy là “Không thầy đố mày làm nên”. Công dưỡng dục thuộc về cha mẹ nhưng công lao dạy dỗ thành người hiểu biết, thành người có nghề nghiệp để sau này sinh sống và định vị cuộc đời mình trước nhân quần xã hội là người thầy. Với tinh thần tôn sư trọng đạo, người Việt quan niệm “Nhất tự vi sư, bán tự vi sư” nghĩa là dạy một chữ là thầy, nửa chữ cũng là thầy – để tỏ lòng tri ân người có công khai tâm cho con người bằng trí thức, bằng chữ.

Đây cũng là dịp cho thầy trò cùng nhau hàn huyên tâm sự mọi chuyện tầng ấy năm xa trường. Vậy hãy mau mau qua chúc tết thầy cô đi nhé','2016-12-09 15:24:46.530601',NULL,NULL,'2016-12-09 15:24:46.530667','action/images/son0004.jpg');
INSERT INTO "core_action" VALUES(22,'Tiễn chân ông táo về trời','Tục cúng Táo quân khởi nguồn từ bếp lửa, tượng trưng cho sự ấm no, sung túc, cung cấp sự sống cho con người bằng việc sưởi ấm, đun chín thức ăn. Đó cũng là hình tượng trong cõi tâm linh về ông Vua Bếp. Sau này, dựa trên các sự tích liên quan đến bếp lửa, dân gian mới sáng tạo ra câu chuyện "hai ông, một bà" nhưng vẫn có ý nghĩa mong muốn một cuộc sống no đủ, bếp gia đình lúc nào cũng rực lửa. 

Cá chép là phương tiện duy nhất có thể đưa Táo Quân về trời. Bởi thế, vào ngày này, sau khi làm lễ xong, các gia đình đều cúng con cá chép rồi đem ra sông hay ra ao thả, ngụ ý “cá hóa long”, nghĩa là cá sẽ hóa rồng, vượt vũ môn, làm phương tiện cho Táo quân cưỡi về trời.

Hãy đưa những chú cá chép đến nơi thuận lợi nhất để chuyến đi về trời của Ông Công, Ông Táo được suôn sẻ và may mắn nha bạn.','2016-12-09 15:29:46.038940',NULL,NULL,'2016-12-09 15:29:46.039000','action/images/Son0005.jpg');
INSERT INTO "core_action" VALUES(23,'Gia đình bạn đã nhờ ai xông nhà tết năm nay chưa?','Người xông nhà, xông đất là người bước chân đầu tiên vào nhà sau phút giao thừa. Do muốn được may mắn, mọi người hay tìm người hợp tuổi đến xông nhà. Người mở hàng có thể là giám đốc công ty, những người thành đạt hoặc chính con cháu, hoặc tự bạn cũng có thể mở hàng cho nhà mình. Điều quan trọng khi chọn người xông nhà, mở hàng đầu năm, đó phải là người vui vẻ, rộng rãi. Tuyệt đối tránh thuê người đến mở hàng, xông đất chỉ để hợp tuổi khi không biết tính cách, năng lực... của họ. 

Vì xông nhà có ý nghĩa lớn về tinh thần và tâm linh như vậy nên hãy lựa chọn đúng người bạn nhé.','2016-12-09 15:33:54.264258',NULL,NULL,'2016-12-09 15:33:54.264378','action/images/son0006.jpg');
INSERT INTO "core_action" VALUES(24,'Đêm nay khai bút đầu năm, Cầu cho năm mới phơi phới điều may','Bạn đã thử cảm giác khai bút đầu năm ngay khi thời khắc chuyển giao năm mới với năm cũ chưa. 
Cái cảm xúc ngồi nắn nót viết những dòng chữ đầu tiên của một năm thật là đặc biệt! Là bạn bạn sẽ viết gì đây ? Đó là những điều tuyệt vời của riêng bạn !

Những gợi ý hay để khai bút đầu xuân nhiều ý nghĩa
{http://tiin.vn/chuyen-muc/hoc/nhung-goi-y-hay-de-khai-but-dau-xuan-nhieu-y-nghia.html}','2016-12-09 15:36:56.020730',NULL,NULL,'2016-12-09 15:36:56.020796','action/images/duan-00003.jpg');
INSERT INTO "core_action" VALUES(25,'Đưa mẹ đi chợ ngày Tết','Con vốn biết thời gian trôi qua nhanh, nhưng đôi khi ngẫm lại vẫn giật mình. Mới chớp mắt mà lại tết nữa rồi. Mẹ hay nói với con câu nói "bảo bối" từ ngoại: “Không có cái tết nào giống cái tết nào”.
Năm con 7 tuổi, điều vui nhất vào dịp tết là được nhận lì xì, hay năm 17 tuổi là được đi chơi cùng bạn bè. Nhưng những năm con lớn hơn tuổi, con chỉ mong đến tết để được đi chợ cùng mẹ, phụ mẹ xách đồ. Đó là khoảnh khắc của bình yên, của ấm áp, của nơi con thuộc về, được nắm tay mẹ thật chặt, chen chúc giữa chợ tết, dù con không còn nhỏ bé gì...
Mẹ cho con đi chợ với mẹ nhé!','2016-12-09 15:41:41.020669','2016-12-09 15:41:25','2017-01-27 05:00:00','2016-12-09 15:41:41.020849','');
INSERT INTO "core_action" VALUES(26,'Bạn đã thử mời một người bạn ngoại quốc về ăn tết Việt chưa?','Tết nguyên đán là một nét văn hóa rất đặc biệt ở Châu Á mà chắc hẳn những người ngoại quốc, đặc biệt là người phương Tây muốn trải nghiệm sự đặc biệt có một không hai này. Hãy mời và tình nguyện giới thiệu cho họ về tết của quê hương Việt Nam của chúng ta. Điều này thật tuyệt vời phải không?','2016-12-09 15:45:12.758573',NULL,NULL,'2016-12-09 15:45:12.758936','action/images/son0007.JPG');
INSERT INTO "core_action" VALUES(27,'Treo ảnh gia đình đón mừng năm mới','Cuối năm bên gia đình trang hoàng lại nhà cửa. Đây là lúc các thành viên trong gia đình bồi đắp thêm tình cảm gia đình gắn bó. Có thể nói, một bức ảnh gia đình chính là phương pháp đơn giản mà hiệu quả nhất để nhắc nhở  mọi người về tình thân, tình thương yêu giữa những thành viên dưới một mái nhà.
Còn gì tuyệt vời hơn nếu Tết này cả nhà có thể chụp cùng nhau những bức ảnh gia đình rồi cùng chọn bức đẹp nhất treo trang trọng giữa phòng khách?
Nếu là các bâc cha mẹ có thể để con cái tự tay lựa vài bức treo chỗ con cái muốn, treo thêm cả trong phòng riêng của con nữa, để nhắc nhở trẻ rằng dẫu có biến cố thăng trầm, con vẫn luôn có mẹ cha ở bên hỗ trợ. Đấy chính là tình máu mủ ruột rà mà một năm bận rộn mưu sinh có lẽ nhiều người quên lãng.
Chi tiết xem thêm:
Những cách treo ảnh đẹp {http://emdep.vn/nha-xinh/nhung-cach-treo-anh-vua-dep-vua-nghe-thuat-ban-se-muon-thu-ngay-20160511150431339.htm}','2016-12-09 16:04:57.921298','2016-12-01 16:00:23','2017-01-26 17:00:00','2016-12-09 16:04:57.921491','action/images/khanh1243.jpg');
INSERT INTO "core_action" VALUES(28,'Em yêu !!! Tết này mình đi cầu duyên !','Tình yêu là một cảm xúc kì diệu mà thượng đế đã ban cho con người, khiến hai con người vốn khác nhau xích lại gần nhau hơn. 
Càng lớn, con người càng mong muốn có được một tình yêu đẹp và bền lâu. Tuy nhiên tình yêu đâu phải ai cũng may mắn tìm được nhau. Có những người suốt cả cuộc đời này vẫn không tìm được thật sự cái gọi là tình yêu. Một số người đã có nó trong tay thì lại không phải ai cũng biết cách nắm giữ, vẫn để tuột mất nhau, tuột mất thứ tình cảm vốn đẹp đẽ đó chỉ trong một thời gian ngắn.
Hãy làm những điều tuyệt tuyệt vời và ý nghĩa để xây đắp tình yêu đôi bạn !
Cùng nhau đi cầu duyên là một điều cực kỳ ý nghĩa đối với những cặp tình nhân. Nếu năm này là lần đầu tiên cầu duyên của đôi bạn thì nó sẽ là một kỷ niệm cực kỳ đáng nhớ. 

Có rất nhiều nơi để các bạn có thể đến cầu duyên và cũng là một điểm đến trong chuyến du xuân như: Chùa Hà (Hà Nội)
Chùa Duyên Ninh (Ninh Bình)
Đền Chử Đồng Tử (Hưng Yên)
Chùa Bát Bửu Phật Đài (TP. HCM)
...
Xem thêm {http://diadiemdulich.com/diem-den/5-ngoi-chua-cau-duyen-noi-tieng-nhat-viet-nam.html}','2016-12-09 16:17:01.594384',NULL,NULL,'2016-12-09 16:17:01.594486','action/images/duan-00004.jpg');
INSERT INTO "core_action" VALUES(29,'Đừng quên lập mục tiêu cho năm mới nhé','Đến hẹn lại lên, những ngày cuối cùng của năm cũ luôn là khoảng thời gian lý tưởng để chúng ta nghiền ngẫm về những điều đã qua và bắt đầu suy nghĩ về những mục tiêu cụ thể cho sự nghiệp trong năm mới.

Theo phản ứng tự nhiên khi nói đến những mục tiêu mới trong công việc, mọi người luôn có thói quen suy luận rằng đó chính là thay đổi công việc. Không hẳn thế, tìm một vị trí mới hoặc tham gia một lĩnh vực nghề nghiệp mới cũng chỉ là một trong vô số các mục tiêu. Vẫn còn có những điều khác, bao gồm trong cả công việc lẫn cuộc sống mà bạn cần phải vạch ra kế hoạch và xác lập mục tiêu cho mình trong năm mới.

Sau đây là một vài gợi ý giúp bạn có từng bước điều chỉnh công việc và cuộc sống thêm phần thuận lợi để hướng đến ước mơ “ vạn sự như ý” ở năm Bính Tuất

Năm mới bắt đầu từ những bước thay đổi nhỏ. Nếu là một người luôn tham công tiếc việc, luôn quay cuồng với hàng đống trách nhiệm và bao giờ cũng trở vể muộn trong suốt bảy ngày trong tuần thì bạn nên tập thay đổi thói quen này dần dần để tránh rước vào mình cảm giác quá tải, mệt mỏi. Hãy bắt đầu thay đổi từ những bước nhỏ.

Ví dụ như: bạn sẽ trở về nhà sớm hơn 30 phút mỗi ngày so với năm cũ, còn nếu bạn vẫn thường xuyên mang việc về nhà thì nên tự đặt ra nguyên tắc “một ngày không công việc trong tuần”. Sự thay đổi nhỏ này sẽ giúp bạn cảm thấy công việc trở nên nhẹ nhàng và cuộc sống thì dễ chịu hơn nhiều so với năm cũ.

Đọc sách nhiều hơn. Hãy dành thời gian đọc sách nhiều hơn, xen kẽ các tài liệu liên quan đến công việc là những tác phẩm giải trí. Dành thời gian đọc nhiều hơn sẽ giúp bạn vừa tiếp thu được những kiến thức mới cần cho nghề nghiệp lại vừa có những giây phút thư giãn bổ ích.

Nếu thường xuyên đi lại bằng các phương tiện giao thông công cộng, bạn nên nhớ luôn để sẳn một quyển sách trong cặp đi làm, hãy tận dụng mọi thời gian qúy báu có được một cách có ý nghĩa bằng việc đọc sách nhiều hơn.

Tham gia các khoá huấn luyện thể thao. Việc này sẽ rất có ích nếu nghề nghiệp của bạn đòi hỏi giao tiếp thường xuyên và yêu cầu cao về ngoại hình. Chọn một môn thể thao yêu thích và phù hợp ghi danh tham gia ngay lập tức. Rèn luyện thân thể không chỉ giúp bạn có một ngoại hình cân đối hơn mà còn giúp bạn có thêm sức khoẻ để đeo đuổi công việc đến cùng.

Cập nhật thông tin mới cho resume. Bạn phải thường xuyên cập nhật những thông tin mới nhất cho resume của mình với những chi tiết cụ thể và rõ ràng. Công việc này sẽ giúp bạn có cơ hội nhìn lại và đánh giá những việc đã làm được, xem lại coi bạn đã thực hiện đúng theo kế hoạch đã định hay không và hơn nữa là sẽ có thêm cơ sở để thiết lập những mục tiêu mới cho mình.

Lên kế hoạch cho những kỳ nghỉ trong năm. Điều này có thể làm cho bạn thấy có vẻ buồn cười. Tuy nhiên, lên kế hoạch trước cho bất cứ công việc gì luôn luôn không thể là chuyện thừa.

Trong thời gian làm việc sắp tới, chúng ta sẽ vô cùng bận rộn, vì thế sẳn sàng gác lại chuyện nghỉ ngơi táo tạo năng lượng nếu chưa có một kế hoạch cụ thể đi đâu làm gì. Làm việc không nghỉ ngơi sẽ không giúp công việc thêm phần hiệu quả mà ngược lại còn đẩy bạn đến trạng thái mệt mỏi, kiệt quệ năng lực.

Một trường hợp khác nữa là nếu bạn có ý định dồn hết phép lại một lần để “chơi” cho đã thì cũng nên thay đổi suy nghĩ, vì bạn sẽ lập tức cảm thấy ngán việc khi quay lại văn phòng sau một thời gian nghỉ ngơi quá dài.

Trau dồi kỹ năng giao tiếp. Giao tiếp là một kỹ năng cần thiết cho mọi nghề nghiệp với những cấp độ khác nhau, đó chính là sức mạnh thu hút sự chú ý và thiết lập mối quan hệ với mọi người.

Thành công chỉ đến được khi bạn có đã xác định được những mục tiêu cụ thể và kế hoạch thực hiện rõ ràng. Với những mục tiêu nhỏ gợi ý trên đây, hy vọng bạn sẽ thành công to trong năm mới.

Nguồn:Dân trí (2006)','2016-12-09 16:39:55.882142',NULL,NULL,'2016-12-09 16:39:55.882209','action/images/duan-00005.jpg');
INSERT INTO "core_action" VALUES(30,'Phong bì nhỏ, bỏ vào bao, trao người lớn','Trở vào nhà sau tràng pháo hoa rực rỡ báo hiệu thời khắc chuyển giao năm cũ - năm mới, cũng là khi bố mẹ thắp hương cúng xong bàn thờ gia tiên. Có giây phút nào hai chữ “gia đình” lại thiêng liêng  thế chăng? Tết này, bạn sẽ mừng tuổi cho bố mẹ mình chứ?

Phong bao đỏ mang theo cả tấm lòng người con với lời cầu chúc ông bà, bố mẹ luôn mạnh khỏe, hạnh phúc bên con cháu. Đặc biệt với những bạn cả năm ít có thời gian bên gia đình, tại sao làm nên những khoảnh khắc thật đáng nhớ? Hãy cùng tấm lì xì nhỏ trao gửi tất cả lời cảm tạ chân thành tới bậc thân sinh vì đã có một năm bươn chải kiếm sống và lo lắng cho bạn. Hãy cho ông bà được cười thật tươi vì đứa cháu của mình đã thật sự trưởng thành. Hãy gieo niềm tin yêu và hi vọng trong gia đình rằng năm mới của bạn sẽ là một năm thành công hơn tất cả những gì họ mong đợi.

Một hành động nhỏ, nhưng ý nghĩa biết chừng nào!','2016-12-10 01:40:03.949272',NULL,NULL,'2016-12-10 07:09:21.499662','action/images/son0008.jpg');
INSERT INTO "core_action" VALUES(31,'Bạn đã bao giờ kinh doanh hoa vào dịp tết chưa?','Mùa xuân là thời điểm trăm hoa tô điểm cho trời đất và cho lòng người. Chẳng biết tự bao giờ, việc ra chợ Tết mua bán hoa, ngắm hoa đã trở thành nét đẹp văn hóa người Việt. Vậy tại sao bạn không thử góp một chút Xuân bằng cách bày lên những đóa hoa tươi đẹp rực rỡ và đem bán cho mọi người?
Thử tưởng tượng rằng bạn là cô chủ nhỏ của những cành hồng đỏ, hồng vàng, hồng nhung, hồng trắng, hồng thắm, nụ vẫn còn e ấp như muốn níu giữ mùa đông; những đóa cúc vàng cúc trắng cánh nhỏ li ti mà đầy thanh cao, sang trọng; này đây nàng lưu ly he hé những cánh kiều diễm, còn kia là dáng tim tím của những bông hoa cẩm chướng thật tươi khi gió Xuân về, … Đứng giữa hàng hoa như đang trôi trong một dòng sông thơm ngát đầy sắc màu, để thấy lòng mình thật tươi trẻ, để thấy mình đang cho đi những hân hoan của mùa Xuân và sẻ chia khát khao được mang thiên nhiên vào nhà của mọi người.
Việc bán hoa tưởng chừng dễ, nhưng các bạn hãy tự chuẩn bị cho mình những tips giúp công việc hiệu quả hơn. Thời gian sắp xếp hợp lí, thường là mười ngày cuối tháng Chạp. Bên cạnh đó,  tìm hiểu trước về thị trường và chuẩn bị kĩ càng về vốn, nguồn nhập hàng, địa điểm và cách thức nhập, bán, …. cực kì quan trọng. Hỏi những người đã từng làm giống bạn cũng là một cách rất hay. Hãy cân nhắc sao cho vừa vặn và cùng tận hưởng thành quả mùa Xuân thôi nào!','2016-12-10 01:43:06.972773',NULL,NULL,'2016-12-10 07:12:26.030773','action/images/son0009.jpg');
INSERT INTO "core_action" VALUES(32,'Mua sách đầu năm','Hãy đến hiệu sách và chọn ngay một quyển sách về lĩnh vực mà bạn yêu thích, một quyển cẩm nang du lịch, hoặc chỉ đơn giản là một quyển truyện tranh,... Hãy tận hưởng một niềm vui mới mỗi ngày bằng cách đọc những gì bạn yêu thích.
Còn gì tuyệt vời hơn, bạn nhận được một lời chúc chứa chan tình cảm trong cuốn sách của một ai đó tặng mình. Cũng còn hạnh phúc nào hơn khi ta dành tặng bố mẹ cùng người thân yêu những cuốn sách mang thông điệp yêu thương đầu năm mới.
Chi tiết tham khảo thêm:
Tựa một số cuốn sách hay đọc ngày Tết {https://www.facebook.com/notes/tikivn/12-t%E1%BB%B1a-s%C3%A1ch-hay-%C4%91%E1%BB%8Dc-d%E1%BB%8Bp-t%E1%BA%BFt/184508731572840/}
Các website mua, đọc sách online hay{https://10hay.com/top-website/top-10-website-mua-sach-doc-sach-online-tot-nhat-viet-nam.html}','2016-12-10 02:29:38.532125','2016-11-30 17:00:00','2017-02-28 02:15:29','2016-12-10 02:29:38.532357','action/images/khanhdz1234.jpg');
INSERT INTO "core_action" VALUES(33,'Cùng bạn bè tham gia một trận thể thao khai xuân đầu năm','Dịp đầu xuân mới luôn gắn liền với những hoạt động " khai xuân".  Đây là một công việc ý nghĩa với mong muốn có được một năm mới may mắn, tốt đệp. Thể thao là một trong những hoạt động thu hút được rất đông người tham gia sau những những ngày nghỉ tết, vừa để tèn luyện sức khỏe và cũng là cơ hội gặp gỡ giao lưu bạn bè đầu năm

Bạn có đang sở hữu cho mình những đồng đội tuyệt vời cùng chơi môn thể thao nào đó. Vậy thì còn chờ gì nữa mà không đặt lịch cho đội mình ngay nhỉ?','2016-12-10 02:42:55.985209',NULL,NULL,'2016-12-10 02:42:55.985273','action/images/son0010.jpg');
INSERT INTO "core_action" VALUES(34,'Hát cho cả nhà nghe đêm giao thừa','Đêm giao thừa là khoảng thời gian thiêng liêng nhất của năm khi cả gia đình sum họp, chuẩn bị đón năm mới với những điều tốt lành, may mắn sẽ đến và tiễn trừ năm cũ với những điều không may đã qua.
Bên cạnh những chùm phá hoa, mâm cỗ truyền thống, những lời chúc, và những chiếc lì xì may mắn. Năm nay sẽ đặc biệt hơn là bạn sẽ chuẩn bị thêm một bài hát ý nghĩa dịp Tết và hát dành tặng các thành viên trong gia đình mình.
Chi tiết xem thêm:
Những bài hát cho đêm giao thừa {http://nhac.vn/playlist-nhung-bai-hat-cho-dem-giao-thua-vol-1-plB4P37?st=sorzJAN}','2016-12-10 02:46:18.634307','2016-11-30 17:00:00','2017-01-26 17:00:00','2016-12-10 02:46:18.634856','');
INSERT INTO "core_action" VALUES(35,'Hóa vàng ngày tết bên gia đình','Theo truyền thống xưa, sau khi mời Tổ tiên về dự 3 ngày Tết với con cháu vào ngày 30 Tết (nếu tháng thiếu thì vào ngày 29 Tết), đến ngày mùng 3 Tết hoặc ngày khai hạ mồng bảy Tết, là ngày cuối cùng, tiệc xuân đã mãn, con cháu lại cáo lễ để tiễn đưa Tổ tiên trở về âm cảnh.Lễ này, tục gọi là “đưa ông bà”, lễ hóa vàng cho Tổ tiên, hay lễ tạ năm mới.

Theo một số chuyên gia văn hóa phương Đông, việc chọn ngày làm lễ hóa vàng tùy thuộc vào mỗi gia đình, chủ yếu từ mùng 3 đến khoảng mùng 10 Tết Nguyên đán. Điều quan trọng nhất là phải có lễ tạ gia tiên, gia thần và chư vị thánh thần, phật. Theo quan niệm dân gian, có lễ tạ thì tấm lòng của chủ nhà mới được người âm chứng giám. Lễ vật dâng cúng trong lễ tạ năm mới gồm: Nhang, hoa, ngũ quả, trầu cau, rượu, đèn nến, bánh kẹo, mâm lễ mặn hoặc chay cùng các món ăn ngày Tết đầy đủ, tinh khiết.

Bạn và gia đình đã định làm lễ hóa vàng vào ngày nào chưa?','2016-12-10 02:52:47.260965',NULL,NULL,'2016-12-10 02:52:47.261032','action/images/Son0011.jpg');
INSERT INTO "core_action" VALUES(36,'Chia sẻ những bức ảnh tết trên quê hương mình với bạn bè','Mỗi một nơi trên tổ quốc Việt Nam lại có một số phong tục, những lễ hội riêng trong địp tết. Cùng nhau chia sẻ những bức ảnh, những câu chuyện đầu xuân sẽ giúp cho mình và bạn bè biết nhiều hơn về tập tục của các vùng khác nhau,  qua đó làm giàu thêm tình yêu đất nước.','2016-12-10 07:19:25.861310',NULL,NULL,'2016-12-10 07:19:25.861369','action/images/son0012a.jpg');
INSERT INTO "core_action" VALUES(37,'Tự tay làm lịch Tết đón năm mới','Mỗi khi năm mới đến mọi người đều cần sắm cho mình những cuốn sổ tay làm lịch, lịch treo tường, lịch để bàn mới. Những bộ lịch handmade hoặc lịch để bàn, lịch dưới dạng sổ tay không chỉ rất đẹp và phong cách khi trang trí nhà, phòng riêng. Bộ lịch handmade còn là món quà tết hoàn hảo nhân dịp năm mới tết đến.

Có rất nhiều mẫu lịch handmade để bàn, mẫu lịch treo tường hoặc thậm chí những quyển lịch làm theo album ảnh của chính bạn. Nguyên liệu để làm một cuốn lịch handmade cũng vô cùng đơn giản và dễ kiếm. Ngoài ra bạn có thể tận dụng giấy bọc quà, album ảnh, sổ cũ, thậm chí thêu chữ thập lên giấy, in trên vải vintage bằng các bộ con dấu handmade… Rất độc đáo phải không?

Chi tiết xem thêm:
Hướng dẫn cách làm các lịch handmade {https://www.youtube.com/watch?v=PTF-96zADNA&list=PLLhVqN6MdbbKbbKtv8zAgBLYTFbExc_MX&index=2}','2016-12-10 07:34:51.818908','2016-12-10 07:34:32','2017-01-27 07:34:35','2016-12-10 07:34:51.819056','action/images/khanh1a23.jpg');
INSERT INTO "core_action" VALUES(38,'Cùng người yêu đi du xuân, lễ hội cầu may','Du lịch là cách tốt nhất để giải trí, thư giãn tinh thần và "hâm nóng" tình yêu đôi lứa. Nếu bạn dự định cùng “người ấy” du xuân dịp Tết này, hãy tăng thêm phần lãng mạn cho chuyến vi vu bằng cách đơn giản sau đây.

Tùy vào sở thích của người yêu mà bạn chọn điểm đến yên tĩnh, thanh bình hay náo nhiệt. Hãy lên danh sách các địa chỉ hấp dẫn nhất rồi sau đó quyết định nơi các bạn thích đến nhất. Thông thường, biển cả, núi rừng, sông nước, ao hồ...là những địa điểm lý tưởng nhất để bạn viếng thăm.

Nếu kinh tế không cho phép, hai bạn cũng có thể chọn mua tour ít điểm đến, ngắn ngày hoặc chọn những nơi có nhiều cây xanh, không khí trong lành và yên tĩnh để tạo cảm giác thú vị cho chuyến đi của mình. Còn gì tuyệt vời hơn khi được cùng nhau thưởng thức một bữa ăn nhẹ ngoài trời trên nền cỏ xanh mướt, hay đi dạo mát ở bãi biển dịu êm...Tất cả những điều ấy sẽ khiến hai bạn thêm hạnh phúc và gắn bó hơn.

Nghe nhạc cùng nhau cũng là cách hay để khơi gợi lại cảm xúc yêu thương giữa hai người. Do đó, dù mua tour hay tự lái xe đi du lịch, các bạn đừng quên chuẩn bị cho mình một đĩa CD với những bản nhạc yêu thích, gắn liền với kỷ niệm đáng nhớ của hai người.

Trên đường khám phá vùng đất mới, nếu bắt gặp chỗ nào có cảnh đẹp và lãng mạn, hai bạn đừng nên bỏ qua cơ hội chụp hình trong bối cảnh thiên nhiên đầy thơ mộng. Có thể nói, cùng nhau trải nghiệm hạnh phúc dưới khung cảnh bình minh và hoàng hôn là những ý tưởng tuyệt vời nhất để ghi lại giây phút hạnh phúc bên nhau mà các "cặp đôi hoàn hảo" sẽ không thể nào quên được.

Nếu muốn tạo niềm vui bất ngờ cho người mình yêu, bạn cũng có thể để một món quà nhỏ ở đâu đó trong hành lý của người ấy. Sau khi tình cờ phát hiện ra nó, chắc chắn “gấu” của bạn sẽ vô cùng cảm động trước tình yêu chân thành từ bạn.

Bạn cũng nên yêu cầu nhà tour đặt riêng hoặc tự đặt bàn trước cho bữa tối lãng mạn tại một nhà hàng ở điểm đến. Còn gì tuyệt vời hơn khi được cùng nhau thưởng thức các món ăn ngon, nâng ly chúc mừng tình yêu và nghỉ ngơi thư giãn sau chuyến đi dài.','2016-12-10 07:37:03.175924',NULL,NULL,'2016-12-10 07:37:03.175986','action/images/Son0013.jpg');
INSERT INTO "core_action" VALUES(39,'Dành chút khoảng riêng ngày tết cho bản thân tự do làm những gì mình thích','Tết không chỉ cho chúng ta thời gian để quây quần bên gia đình, họ hàng, bạn bè. Tết còn cho ta thời gian để thực hiện những ước muốn nhỏ nhoi, hay theo đuổi một đam mê nho nhỏ. Bởi vậy dù có bận rộn hày nhiều lịch hẹn thế nào, bạn hãy nhớ dành ra một ít thời gian và không gian riêng để tận hưởng cuộc sống này bạn nhé.','2016-12-10 07:47:59.626527',NULL,NULL,'2016-12-10 07:47:59.626589','action/images/son0014.jpg');
INSERT INTO "core_action" VALUES(41,'Bạn đã biết mình sẽ mặc gì vào những ngày đầu xuân chưa?','Ngày Tết, bạn tạm rời xa những bộ công sở gò bó để thể hiện gu thời trang của mình. 3 gợi ý dưới đây giúp bạn xinh tươi, rạng rỡ hơn trong dịp năm mới.

Những bộ đồ thanh lịch, sang trọng là lựa chọn an toàn cho bạn khi sum họp gia đình, khi đi chúc Tết họ hàng, người thân. Vì thế, một chiếc váy liền và áo choàng - cách mix đồ kinh điển - sẽ giúp bạn ghi điểm. Trong quan niệm của người Việt thì màu đỏ mang lại may mắn, vì thế, trang phục có màu đỏ, hoặc màu sắc tươi sáng cũng khiến những ngày đầu năm thêm tươi vui, rộn ràng.

Thay vì bận rộn tới tấp với công việc, ngày Tết là dịp bạn có thể thảnh thơi đi chơi đây đó. Lúc này, chân váy xòe, hoặc váy mullet và áo cardigan nhẹ nhàng khiến bạn như mang theo làn gió xuân tươi trẻ bên mình. Một đôi giầy bệt cũng giúp bạn thoải mái, thư thái hơn.

Chi tiết xem thêm{http://giaitri.vnexpress.net/tin-tuc/thoi-trang/lam-dep/3-go-i-y-trang-phu-c-cho-nga-y-te-t-2422970.html}','2016-12-10 07:52:47.456258',NULL,NULL,'2016-12-10 08:05:10.252208','action/images/son0015.jpg');
INSERT INTO "core_action" VALUES(42,'Đi chợ hoa ngày Tết','Khám phá chợ hoa ngày Tết là một trong những nét độc đáo của người dân Việt Nam mỗi dịp Tết đến xuân về. Dạo qua chợ hoa ngày Tết để cảm nhận được không khí xuân đang về, chụp ảnh làm kỉ niệm hay lựa chọn những chậu hoa đẹp nhất về trang trí cho gia đình ngày Tết.
Chi tiết xem thêm:
Cách chọn hoa-cây cảnh ngày tết{https://www.youtube.com/watch?v=R1W-6sxrs6Y}
Các chợ hoa nổi tiếng Hà Nội{http://dulich.vnexpress.net/tin-tuc/viet-nam/ha-noi/6-cho-hoa-tet-noi-tieng-cua-ha-noi-3352276.html}
Các chợ hoa nổi tiếng Sài Gòn{http://dulich9.com/nhung-cho-hoa-ngay-tet-noi-tieng-o-sai-gon-nhon-nhip-va-doc-dao.html}','2016-12-10 08:06:33.493257','2016-11-30 17:00:00','2017-01-30 17:00:00','2016-12-10 08:06:33.493398','action/images/khanhzc1.jpg');
INSERT INTO "core_action" VALUES(43,'Một chuyến đi chơi xa ngày tết cùng đám bạn cũ','Một chuyến đi vào đầu năm mới sẽ làm tinh thần thêm sảng khoái hơn rất nhiều, khi đi cùng đám bạn cũ mọi người có thể nói chuyện với nhau nhiều hơn và kể cho nhau nghe những câu chuyện một năm qua
  Đầu năm thường những địa điểm được chọn du lịch nhiều nhất là chùa chiền hay là những địa điểm tâm linh, nhưng với nhịp sống xã hội hay là phù hợp với túi tiền thì cách đi là của bạn
  Có nhiều típ để đi du lịch đầu năm mọi người tham khảo qua nhé, nhớ hãy làm những vị khách thật văn minh và đem cho và nhận tất cả những niềm vui năm mới.

{http://dulich9.com/du-lich-tet-nguyen-dan-2017-nen-di-dau-lay-loc-dau-nam.html}','2016-12-10 08:10:31.623242',NULL,NULL,'2016-12-10 08:10:31.623329','action/images/du-lich-tet-trong-nuoc2.jpg');
INSERT INTO "core_action" VALUES(44,'Tập tành kinh doanh ngày tết','Với người trẻ thì ai cũng có máu kinh doanh và kiếm tiền để tiêu xài trong dịp tết. Hãy thứ tự đứng ra hoặc cùng với mấy đứa bạn hùn vốn kinh doanh một thứ nào đó đi nào. Việc buôn bán sẽ mang cho bạn nhiều kinh nghiệm hay ho trong cuộc sống và học tập.
  Bán hoa, bán quần áo, bánh kẹo, quần áo... hãy thử làm ông bà chủ thôi nào

chi tiết xem tại {http://www.phoithai.com/kinh-doanh-gi-ngay-tet-von-it-lai-nhieu.html}','2016-12-10 08:26:04.889218',NULL,NULL,'2016-12-10 08:26:04.889319','action/images/Manhhv4.jpg');
INSERT INTO "core_action" VALUES(45,'Đi từ thiện vùng cao dịp giáp Tết','Thật hạnh phúc và may mắn cho những ai được quây quần bên gia đình, được ăn ngon mặc ấm. Nhưng đâu đó bên cạnh chúng ta vẫn còn những mảnh đời kém may mắn, những hoàn cảnh khó khăn và không phải ai cũng được hưởng những cái điều giản dị đó.
Rủ bạn bè xung quanh cùng quyên góp quần áo cũ không dùng tới hay, ủng hộ một phần tài tài chính cá nhân dịp cuối năm hay chỉ là những cuốn sách cũ . Chuẩn bị tươm tất những món quà ý nghĩa muốn gửi, lên đường bắt đầu một chuyến thiện nguyện đầy ý nghĩa dịp giáp Tết nào!
Chi tiết xem thêm:','2016-12-10 08:53:27.247408','2016-12-01 08:53:03','2017-01-31 11:00:00','2016-12-10 08:53:27.247546','action/images/khanhok1.JPG');
INSERT INTO "core_action" VALUES(46,'Chụp ảnh đại gia đình vào năm mới','Tết là dịp để gia đình đoàn viên, mọi người cùng sum họp đầy đủ cũng là dịp để đánh dấu mọi người thêm tuổi mới và nhìn lại những thay đổi của một năm đã qua. Công nghệ khoa học phát triền không chỉ làm việc có những bức ảnh dễ dàng hơn mà còn lưu vào để mỗi lúc nhớ nhà mình có thể đem ra ngắm.
  Một bức ảnh đại gia đình đầu năm là một việc cần thiết để lưu giữ những kỉ niệm đẹp đẽ ngày tết. Có những tấm ảnh đã lâu mình chưa nhìn lại hay có bức ảnh được treo trọng trên tường nhà những bức ảnh như thế đều là những kỉ niệm, những sự thay đổi đáng nhớ của ngày tết. Mỗi khi ngắm nhìn những bức ảnh tình yêu thương gia đình lại ngập tràn và nhớ về những ngày tết năm nao.','2016-12-10 08:53:32.334533',NULL,NULL,'2016-12-10 08:53:32.334594','action/images/manhhv3.jpg');
INSERT INTO "core_action" VALUES(47,'Ngắm nhìn phố phường ngày tết vào buổi sáng sớm','Những buổi sáng sớm trong những ngày tết với cái khí trời se se lạnh, cải nét vắng vẻ chưa đủ rộn rã như lúc mặt trời lên đến đỉnh đầu cộng với những tia sáng lấp ló của ông mặt trời lên thật đang là một khung cảnh yêu bình cho chúng ta chiêm ngưỡng. Và đặc biệt ở những vùng quê, xa xa đâu đó có tiếng rao muối, rao vôi vọng lại...','2016-12-10 08:56:16.767118',NULL,NULL,'2016-12-10 08:56:16.767169','action/images/son0017.jpg');
